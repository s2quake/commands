{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "build-script",
            "type": "PowerShell",
            "request": "launch",
            "script": "${workspaceFolder}/build.ps1",
            "cwd": "${cwd}"
        },
        {
            "name": "launch-parse",
            "type": "coreclr",
            "request": "launch",
            "program": "${workspaceFolder}/JSSoft.Commands.Parse/bin/Debug/net8.0/parse.dll",
            "args": "--version",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/JSSoft.Commands.Parse/bin/Debug/net8.0",
            "stopAtEntry": false,
            "internalConsoleOptions": "neverOpen"
        },
        {
            "name": "launch-invoke",
            "type": "coreclr",
            "request": "launch",
            "program": "${workspaceFolder}/JSSoft.Commands.Invoke/bin/Debug/net8.0/invoke.dll",
            "args": "list --help",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/JSSoft.Commands.Invoke/bin/Debug/net8.0",
            "stopAtEntry": false,
            "internalConsoleOptions": "neverOpen"
        },
        {
            "name": "launch-repl",
            "type": "coreclr",
            "request": "launch",
            "program": "${workspaceFolder}/JSSoft.Commands.Repl/bin/Debug/net8.0/repl.dll",
            "args": "",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/JSSoft.Commands.Repl/bin/Debug/net8.0",
            "stopAtEntry": false,
            "internalConsoleOptions": "neverOpen"
        },
        {
            "name": "launch-sets",
            "type": "coreclr",
            "request": "launch",
            "program": "${workspaceFolder}/JSSoft.Commands.Sets/bin/Debug/net8.0/sets.dll",
            "args": "--help",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/JSSoft.Commands.Sets/bin/Debug/net8.0",
            "stopAtEntry": false,
            "internalConsoleOptions": "neverOpen"
        },
        {
            "name": "launch-app-ui",
            "type": "coreclr",
            "request": "launch",
            "program": "${workspaceFolder}/JSSoft.Commands.AppUI/bin/Debug/net8.0/app-ui.dll",
            "cwd": "${workspaceFolder}/JSSoft.Commands.AppUI/bin/Debug/net8.0",
            "stopAtEntry": false
        }
    ]
}