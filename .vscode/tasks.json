{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "tasks": [
        {
            "type": "dotnet",
            "task": "build",
            "problemMatcher": [
                "$msCompile"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "label": "dotnet: build"
        },
        {
            "type": "dotnet",
            "task": "clean",
            "problemMatcher": [
                "$msCompile"
            ],
            "group": "none",
            "label": "dotnet: clean"
        }
    ]
}